{
    "toy": false,
    "normalizer": "robust_scale",
    "step": 256,
    "test_frac": 0.2,
    "relabel_classes": {"AFIB": "AF", "AFL": "AF"},

    "conv_subsample_lengths": [2, 2, 2, 2, 2, 2, 2, 2],
    "conv_filter_length": 16,
    "conv_num_filters": 32,
    "conv_init": "he_normal",
    "conv_activation": "relu",
    "conv_dropout": 0.25,
    "num_skip": 2,
    "is_correct_resnet": false,

    "learning_rate": 0.001,
    "optimizer": "adam",
    "batch_size": 128,

    "blacklist_path": "./data/label_review",
    "data_path": "./data/batches",
    "FOLDER_TO_SAVE": "./saved/"
}
