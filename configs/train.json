{
    "toy": false,
    "normalizer": "robust_scale",
    "step": 256,
    "test_frac": 0.2,
    "relabel_classes": {"AFIB": "AF", "AFL": "AF"},

    "conv_subsample_lengths": [2, 2, 2, 2, 2, 2, 2, 2],
    "conv_activation": "relu",
    "conv_filter_length": 16,
    "conv_num_filters": 32,
    "conv_init": "he_normal",
    "conv_activation": "relu",
    "conv_dropout": 0,
    "num_skip": 2,

    "learning_rate": 0.01,
    "momentum": 0.9,
    "decay": 1e-4,
    "clipnorm": 5,
    "batch_size": 128,

    "blacklist_path": "./data/label_review",
    "data_path": "./data",
    "FOLDER_TO_SAVE": "./saved/"
}
