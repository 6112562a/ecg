{
    "toy": false,
    "normalizer": "robust_scale",
    "step": 256,
    "test_frac": 0.2,
    "epi_ext": ".episodes.json",

    "gaussian_noise": 0.001,
    "conv_subsample_lengths": [2, 2, 2, 2, 2, 2, 2, 2],
    "conv_activation": "relu",
    "conv_filter_length": 16,
    "conv_num_filters": 64,
    "conv_init": "he_normal",
    "conv_activation": "leaky_relu",
    "conv_dropout": 0.25,

    "learning_rate": 0.001,
    "clipnorm": 1,
    "batch_size": 128,

    "blacklist_path": "./data/label_review",
    "FOLDER_TO_SAVE": "./saved/"
}
