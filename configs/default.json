{
    "toy": false,
    "normalizer": "robust_scale",
    "seed": 2016,
    "step": 256,
    "val_frac": 0.2,
    "extension": ".episodes.json",

    "gaussian_noise": 0.001,
    "conv_subsample_lengths": [2, 2, 2, 2, 2, 2, 2, 2],
    "conv_activation": "relu",
    "conv_filter_length": 16,
    "conv_num_filters": 64,
    "conv_init": "he_normal",
    "conv_activation": "relu",
    "conv_dropout": 0.25,

    "learning_rate": 0.001,
    "clipnorm": 1,
    "use_batch_norm": true,

    "FOLDER_TO_SAVE": "./saved/"
}
