{
    "version": 2,

    "conv_subsample_lengths": [2, 2, 2, 5, 5],
    "conv_activation": "relu",
    "conv_filter_length": 16,
    "conv_num_filters": 64,
    "conv_init": "he_normal",
    "conv_activation": "relu",
    "conv_l2_penalty": 0.01,
    "conv_dropout": 0,

    "recurrent_layers": 1,
    "recurrent_hidden": 64,
    "recurrent_type": "LSTM",
    "recurrent_activation": "relu",
    "recurrent_is_bidirectional": true,

    "dense_layers": 1,
    "dense_hidden": 64,
    "dense_activation": "relu",
    "dense_init": "he_normal",
    "dense_l2_penalty": 0.01,
    "dense_dropout": 0,

    "learning_rate": 0.001,

    "FOLDER_TO_SAVE": "./saved/"
}